class Animal:
    def __init__(self, nombre):
        self.nombre = nombre
    def emitir_sonido(self):
        print("Hace un sonido")
class Perro(Animal):
    def emitir_sonido(self):
        print("Guau!")
class Gato(Animal):
    def emitir_sonido(self):
        print("Miau!")
if __name__ == "__main__": 
    mi_perro = Perro("Firulais")
    mi_gato = Gato("Michi")
    mi_perro.emitir_sonido()
    mi_gato.emitir_sonido()
